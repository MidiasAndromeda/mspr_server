{
    "name": "Launch app",
    "type": "node",
    "request": "launch",
    "args": [
        "src/main.ts"
    ],
    "runtimeArgs": [
        "-r",
        "ts-node/register",
        "-r"
    ],
    "cwd": "${workspaceRoot}",
    "protocol": "inspector",
    "internalConsoleOptions": "openOnSessionStart",
    "env": {
        "NODE_ENV": "local",
        "NODE_PORT": "9000"
    },
    "sourceMaps": true,
    "console": "internalConsole",
    "outputCapture": "std"
}